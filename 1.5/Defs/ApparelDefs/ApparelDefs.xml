<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <ThingDef  Abstract="True" Name="VC_Suit" ParentName="VC_Apparel">
	  <generateAllowChance>0</generateAllowChance>
	  <drawGUIOverlay>true</drawGUIOverlay>
    <statBases><!--基础数据-->
      <WorkToMake>3000</WorkToMake><!--工作量-->
      <MaxHitPoints>100</MaxHitPoints><!--最大耐久-->
      <SellPriceFactor>0.1</SellPriceFactor><!--交易价格系数-->
      <Flammability>0.6</Flammability>
      <Beauty>-3</Beauty>
      <Mass>2</Mass>
      <ArmorRating_Sharp>0.05</ArmorRating_Sharp><!--锐器抗性-->
      <ArmorRating_Blunt>0.05</ArmorRating_Blunt><!--钝器抗性-->
      <ArmorRating_Heat>0.05</ArmorRating_Heat><!--能量抗性-->
      <Insulation_Cold>4</Insulation_Cold><!--防寒-->
      <Insulation_Heat>3</Insulation_Heat><!--耐热--> 
	  <EquipDelay>1</EquipDelay><!--穿备速度-->
  </statBases>
	<tickerType>Never</tickerType>
  </ThingDef>

