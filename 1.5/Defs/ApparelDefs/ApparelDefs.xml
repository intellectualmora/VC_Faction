  <ThingDef ParentName="VC_Suit">
    <defName>VC_Civilian_Farmer_upper</defName>
    <description>农民衬衫</description>
    <label>农民衬衫</label>
	  <generateAllowChance>0</generateAllowChance>
  	  <graphicData>
        <texPath>Apparel/Civilian_Farmer_upper/Civilian_Farmer_upper</texPath><!--数据路径-->
        <graphicClass>Graphic_Single</graphicClass><!--图形类别-->
        <drawSize>2</drawSize><!--图形大小-->
		  <onGroundRandomRotateAngle>35</onGroundRandomRotateAngle>
	  </graphicData>
    <apparel><!--服装-->
      <developmentalStageFilter>Adult</developmentalStageFilter>
      <bodyPartGroups><!--覆盖部分-->
        <li>Torso</li><!--躯干-->
        <li>Shoulders</li><!--肩膀-->
        <li>Arms</li><!--手臂-->
      </bodyPartGroups>
      <wornGraphicPath>Apparel/Civilian_Farmer_upper/Civilian_Farmer_upper</wornGraphicPath><!--图像路径-->
      <layers><!--图层-->
        <li>OnSkin</li><!--外套-->
      </layers>
      <tags><!--标签-->
        <li>VC_Villager</li>
      </tags>
      <defaultOutfitTags><!--默认服装标签-->
        <li>VC_Villager</li>
        <li>Villager</li>
      </defaultOutfitTags>
    </apparel>
  </ThingDef>

